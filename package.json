{
  "name": "clean",
  "version": "1.0.0",
  "description": "A sandbox to test building a NodeJS/ReactJS/Redux/Express/Webpack setup",
  "main": "index.js",
  "scripts": {
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "npm run build",
    "dev": "concurrently --kill-others \"npm run dev-api\" \"npm run dev-server\" \"npm run webpack-dev-server\"",
    "dev-api": "NODE_ENV=development nodemon --exec babel-node ./server/api.js",
    "dev-server": "NODE_ENV=development nodemon --exec babel-node ./server/server.js",
    "webpack-dev-server": "NODE_ENV=development babel-node ./server/webpack-dev-server.js",
    "start": "concurrently --kill-others \"npm run start-api\" \"npm run start-server\"",
    "start-api": "NODE_ENV=production babel-node ./server/api.js",
    "start-server": "NODE_ENV=production babel-node ./server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darrindickey/clean.git"
  },
  "author": "Darrin T. Dickey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/darrindickey/clean/issues"
  },
  "homepage": "https://github.com/darrindickey/clean#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2017": "^6.22.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "nodemon": "^1.11.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "dependencies": {
    "autoprefixer": "^6.7.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "bcrypt": "^1.0.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "bookshelf": "^0.10.3",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.26.1",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "http-proxy": "^1.16.2",
    "knex": "^0.12.6",
    "mysql": "^2.13.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "postcss-loader": "^1.2.2",
    "pretty-error": "^2.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "sequelize": "^3.30.2",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
